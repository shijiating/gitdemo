function sendXiazaiInfo() {

    var os = $.trim($('#os').val());

    var banben = $.trim($('#softname').val());

    var userName = $.trim($('#userName').val());

    var userMail = $.trim($('#userMail').val());

    var userTel = $.trim($('#userTel').val());

    var verify = $.trim($('#verify').val());

    var company = $.trim($('#company').val());

    var job = $.trim($('#job').val());



    if (!CheckData2("çœŸå®å§“å", userName, "pname", true)) {

        getFocus('userName');

        return false;

    }

    if (!CheckData2("ç”µå­é‚®ç®±", userMail, "email", true)) {

        getFocus('userMail');

        return false;

    }

    if (!CheckData2("æ‚¨çš„è”ç³»ç”µè¯(æ‰‹æœº)", userTel, "phone", true)) {

        getFocus('userTel');

        return false;

    }



    if (!CheckData("éªŒè¯ç ", verify, "pname", true)) {

        getFocus('verify');

        return false;

    }



    if (!CheckData2("å•ä½åç§°", company, "pname", true)) {

        getFocus('company');

        return false;

    }

    if (!CheckData2("å·¥ä½œå²—ä½", job, "pname", false)) {

        getFocus('job');

        return false;

    }



    $.post('verifysms.php', { smsuser: userName, mobile: userTel, verify: verify, action: 'submitinfo' }, function(data) {

        if (data.code == 105) {

            $('.xiazaiInfoContentForm').html('<p class="fs20 pt50 fwb">æ­£åœ¨å¤„ç†ï¼Œè¯·ç¨åï¼</p>');

            $.post('liuyan.php', { os: os, banben: banben, userName: userName, userMail: userMail, userTel: userTel, verify: verify, company: company, job: job, ly_type: 'xiazai' }, function(data) {

                console.log(data);

                $('.xiazaiInfoContentForm').html('<p class="fs20 pt50 fwb">æ„Ÿè°¢æ‚¨å¯¹Navicatä¸­æ–‡ç‰ˆçš„æ”¯æŒï¼</p><p class="pt10 fs16">æˆ‘ä»¬ä¼šå°½å¿«ä¸ºæ‚¨å›å¤ã€‚</p>');

            });

        } else {

            alert('éªŒè¯ç é”™è¯¯ï¼ï¼ï¼')

            getFocus('verify');

        }



    });



}





function sendGoumaiInfo() {

    var banben = $('.products').filter('.active').attr('lang');

    var number = $('#selNum').val();

    var company = $.trim($('#company').val());

    var userName = $.trim($('#userName').val());

    var userTel = $.trim($('#userTel').val());

    var userMail = $.trim($('#userMail').val());

    var userHobby = $.trim($('#userHobby').val());

    var yzm = $.trim($('#yzm').val());

    var code = getCookie('yzm');

    yzm = yzm.toLocaleLowerCase();



    if (!CheckData2("å…¬å¸åç§°", company, "pname", true)) {

        getFocus('gongsiName');

        return false;

    }



    if (!CheckData2("è”ç³»äºº", userName, "pname", true)) {

        getFocus('username');

        return false;

    }



    if (!CheckData2("æ‰‹æœºå·ç ", userTel, "phone", true)) {

        getFocus('tel');

        return false;

    }



    if (!CheckData2("ç”µå­é‚®ç®±", userMail, "email", true)) {

        getFocus('regmail');

        return false;

    }



    if (!CheckData2("éœ€æ±‚æè¿°", userHobby, "pname", false)) {

        getFocus('miaoshu');

        return false;

    }



    if (yzm != code) {

        alert('éªŒè¯ç é”™è¯¯ï¼');

        getFocus('yzm');

        return false;

    }



    layer.load();



    $.post('liuyan.php', { banben: banben, number: number, userName: userName, userTel: userTel, userMail: userMail, company: company, userHobby: userHobby, yzm: yzm, ly_type: 'xuqiu' }, function(data) {

        console.log(data);

        layer.closeAll();

        layer.msg('æ„Ÿè°¢æ‚¨å¯¹CorelDRAWçš„æ”¯æŒï¼<br>æˆ‘ä»¬ä¼šå°½å¿«ä¸ºæ‚¨å›å¤ã€‚', { icon: 1 });

        window.location.reload();

    });

}







/*å…¬å…±jsä»£ç  begin*/

function searchUrl() {

    var reg = /^(\s*)$/g;

    if (reg.test(document.getElementById("searchValueId").value))

        var res = '\'\'';

    else

        res = document.getElementById("searchValueId").value;

    window.location.href = 'http://' + location.hostname + '/Tag/' + res;

}



function searchUrl2() {

    var reg = /^(\s*)$/g;

    if (reg.test(document.getElementById("searchSupport").value))

        var res = '\'\'';

    else

        res = document.getElementById("searchSupport").value;

    window.location.href = 'http://' + location.hostname + '/Tag/' + res;



}



function addCookie(objName, objValue, objHours) { //æ·»åŠ cookie

    var str = objName + "=" + escape(objValue);

    if (objHours > 0) { //ä¸ºæ—¶ä¸è®¾å®šè¿‡æœŸæ—¶é—´ï¼Œæµè§ˆå™¨å…³é—­æ—¶cookieè‡ªåŠ¨æ¶ˆå¤±

        var date = new Date();

        var ms = objHours * 3600 * 1000;

        date.setTime(date.getTime() + ms);

        str += "; expires=" + date.toGMTString();

    }

    document.cookie = str;

}



function getCookie(objName) { //è·å–æŒ‡å®šåç§°çš„cookieçš„å€¼

    var arrStr = document.cookie.split("; ");

    for (var i = 0; i < arrStr.length; i++) {

        var temp = arrStr[i].split("=");

        if (temp[0] == objName)

            return unescape(temp[1]);

    }

}



function CheckData(strTitle, strInput, strType, blNeed) //jsæ­£åˆ™åˆ¤æ–­

{

    switch (strType)

    {

        case "pname":

            var pattern = /^.{2,200}$/;

            var msg = "\n\næ ¼å¼è¦æ±‚: 2-200ä¸ªå­—ç¬¦    ";

            break;

        case "email":

            var pattern = /^[a-zA-Z0-9_\-\.]+@([a-zA-Z0-9\-]+\.)+[a-zA-Z]{2,5}$/;

            var msg = "";

            if (strInput != "")

            {

                var msg = "\n\n- æ‚¨å¡«å†™çš„ç”µå­é‚®ç®±æ˜¯ " + strInput;

                msg += "   \n\n- è¯·ä»”ç»†æ ¸å¯¹æ‹¼å†™æ˜¯å¦æ­£ç¡®ã€‚  ";

            } else

            {

                var msg = "\n\n- ç”µå­é‚®ç®±ä¸èƒ½ä¸ºç©ºï¼";

            }

            break;

        case "telnum":

            var pattern = /^1[0-9]{10}$|^106[0-9]{9,12}$/;

            var msg = "";

            break;

        case "postcode":

            var pattern = /^[0-9]{6}$/;

            var msg = "\n\næ ¼å¼è¦æ±‚: 6ä½æ•°å­—    ";

            break;

        case "number":

            var pattern = /^[1-9]{1}[0-9]{0,4}$/;

            var msg = "\n\næ ¼å¼è¦æ±‚: æ•°å­—    ";

            break;

        case "tax_account":

            var pattern = /^[\w]{10,20}$/;

            var msg = "\n\næ ¼å¼è¦æ±‚: 20ä½ä»¥å†…å­—æ¯æ•°å­—    ";

            break;

    }



    strInput = strInput.replace(/(^\s*)|(\s*$)/g, "");

    if ((strInput == "") && blNeed)

    {

        alert("è¯·æ‚¨å¡«å†™" + strTitle + "ã€‚  " + msg);

        return false;

    }



    if (strInput != "")

    {

        var result = strInput.match(pattern);

        if (result == null)

        {

            alert("è¯·æ‚¨å¡«å†™æœ‰æ•ˆçš„" + strTitle + "ã€‚  " + msg);

            return false;

        }

    }

    return true;

}



