/*éè´­ä¹°è¡¨å•çŸ­ä¿¡ å–çš„idä¸ä¸€æ ·

typeid 3ç”³è¯·ä¸‹è½½ 4ç”³è¯·è´­ä¹° 8è´­ä¹°éªŒè¯*/

function sendverify(typeid) {

    var smsuser = $.trim($('#userName').val());

    var userTel = $.trim($('#userTel').val());

    var yzm = $.trim($('#yzm').val());





    if (!CheckData("å§“å", smsuser, "pname", true)) {

        getFocus('userName');

        return false;

    }

    if (!CheckData("è”ç³»ç”µè¯", userTel, "telnum", true)) {

        getFocus('userTel');

        return false;

    }

    if (!CheckData2("éªŒè¯ç ", yzm, "pname", true)) {

        getFocus('yzm');

        return false;

    }



    $.post('verifysms.php', { smsuser: smsuser, mobile: userTel, yzm: yzm, typeid: typeid, action: 'sendmsg' }, function(data) {

        if (data.code == 103) {

            sendverifySecond($('#sendverifyBtn'), 90);

            changeYzm();

        } else if (data == 'yzm') {

            alert('éªŒè¯ç é”™è¯¯ï¼');

            getFocus('yzm');

        } else {

            alert(data.msg);

        }

    });



}

/*è´­ä¹°è¡¨å•çŸ­ä¿¡ å–çš„idä¸ä¸€æ ·

typeid 3ç”³è¯·ä¸‹è½½ 4ç”³è¯·è´­ä¹° 8è´­ä¹°éªŒè¯*/

function sendverifyGM(typeid) {

    var smsuser = $.trim($('#username').val());

    var userTel = $.trim($('#tel').val());

    var yzm = $.trim($('#yzm').val());



    if (!CheckData("å§“å", smsuser, "pname", true)) {

        getFocus('username');

        return false;

    }



    if (!CheckData("è”ç³»ç”µè¯", userTel, "telnum", true)) {

        getFocus('tel');

        return false;

    }

    if (!CheckData2("éªŒè¯ç ", yzm, "pname", true)) {

        getFocus('yzm');

        return false;

    }



    $.post('verifysms.php', { smsuser: smsuser, mobile: userTel, yzm: yzm, typeid: typeid, action: 'sendmsg' }, function(data) {

        if (data.code == 103) {

            sendverifySecond($('#sendverify'), 90);

            changeYzm();

        } else if (data == 'yzm') {

            alert('éªŒè¯ç é”™è¯¯ï¼');

            getFocus('yzm');

        } else {

            alert(data.msg);

        }

    });

}





function sendverifySecond(ele, time) {

    ele.attr('disabled', true);

    t = setInterval(function() {

        if (time <= 0) {

            clearInterval(t);

            ele.removeAttr('disabled').html('è·å–æ‰‹æœºéªŒè¯ç ');

            return;

        }

        ele.html('è·å–æ‰‹æœºéªŒè¯ç (' + time + 's)');

        --time;

    }, 1000);

}



function checkverify() {

    var smsuser = $.trim($('#userName').val());

    var userTel = $.trim($('#userTel').val());

    var verify = $.trim($('#verify').val());



    if (!CheckData("å§“å", smsuser, "pname", true)) {

        getFocus('userName');

        return false;

    }

    if (!CheckData("è”ç³»ç”µè¯", userTel, "telnum", true)) {

        getFocus('userTel');

        return false;

    }

    if (!CheckData("æ‰‹æœºéªŒè¯ç ", verify, "pname", true)) {

        getFocus('verify');

        return false;

    }



    $.post('verifysms.php', { smsuser: smsuser, mobile: userTel, verify: verify, action: 'submitinfo' }, function(data) {

        if (data.code == 105) {

            $('.huodongYZ').hide();

            $('#orderForm').show();

            $('#username').val(smsuser);

            $('#tel').val(userTel);

        } else {

            alert(data.msg);

            getFocus('verify');

        }

    });

}





