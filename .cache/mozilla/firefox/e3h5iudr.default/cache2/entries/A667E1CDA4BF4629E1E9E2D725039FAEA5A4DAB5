function CheckData2(strTitle, strInput, strType, blNeed) //jsæ­£åˆ™åˆ¤æ–­

{

    switch (strType)

    {

        case "pname":

            var pattern = /^.{2,200}$/;

            var msg = "\n\næ ¼å¼è¦æ±‚: 2-200ä¸ªå­—ç¬¦    ";

            break;

        case "email":

            var pattern = /^[a-zA-Z0-9_\-\.]+@([a-zA-Z0-9\-]+\.)+[a-zA-Z]{2,5}$/;

            var msg = "";

            if (strInput != "")

            {

                var msg = "\n\n- æ‚¨å¡«å†™çš„ç”µå­é‚®ç®±æ˜¯ " + strInput;

                msg += "   \n\n- è¯·ä»”ç»†æ ¸å¯¹æ‹¼å†™æ˜¯å¦æ­£ç¡®ã€‚  ";

            } else

            {

                var msg = "\n\n- ç”µå­é‚®ç®±ä¸èƒ½ä¸ºç©ºï¼";

            }

            break;

        case "msn":

            var pattern = /^[a-zA-Z0-9_\-\.]+@([a-zA-Z0-9\-]+\.)+[a-zA-Z]{2,5}$/;

            var msg = "";

            if (strInput != "")

            {

                var msg = "\n\n- æ‚¨å¡«å†™çš„MSNæ˜¯ " + strInput;

                msg += "   \n\n- è¯·ä»”ç»†æ ¸å¯¹æ‹¼å†™æ˜¯å¦æ­£ç¡®ã€‚  ";

            }

            break;

        case "phone":

            var pattern = /^1[0-9]{10}$|^106[0-9]{9,12}$/;

            var msg = "\n\næ ¼å¼å¦‚ä¸‹ï¼š138********";

            break;

        case "tel":

            var pattern = /^[0-9]{3,4}-[0-9]{7,8}$/;

            var msg = "\n\næ ¼å¼å¦‚ä¸‹ï¼š000-0000000/0000-00000000";

            break;

        case "postcode":

            var pattern = /^[0-9]{6}$/;

            var msg = "\n\næ ¼å¼è¦æ±‚: 6ä½æ•°å­—    ";

            break;

        case "qq":

            var pattern = /^[1-9][0-9]{4,11}$/;

            var msg = "\n\næ ¼å¼è¦æ±‚ï¼š5ä½æˆ–12ä½æ•°å­—";

            break;

    }



    strInput = strInput.replace(/(^\s*)|(\s*$)/g, "");

    if ((strInput == "") && blNeed)

    {

        alert("è¯·æ‚¨å¡«å†™" + strTitle + "ã€‚  " + msg);

        return false;

    }



    if (strInput != "")

    {

        var result = strInput.match(pattern);

        if (result == null)

        {

            alert("è¯·æ‚¨å¡«å†™æœ‰æ•ˆçš„" + strTitle + "ã€‚  " + msg);

            return false;

        }

    }

    return true;

}



function jsaction2()

{



    if (!CheckData2("çœŸå®å§“å", document.getElementById('trueName').value, "pname", true)) {

        getFocus('trueName');

        return false;

    }



    if (!CheckData2("æ‰‹æœºå·ç ", document.getElementById('mobile').value, "phone", true)) {

        getFocus('mobile');

        return false;

    }



    if ($('#chequeFront').val() != undefined && !CheckData2("å‘ç¥¨æŠ¬å¤´", $('#chequeFront').val(), "pname", true)) {

        getFocus('chequeFront');

        return false;

    }

    if ($('#invoice_tax_account').val() != undefined && !CheckData2("çº³ç¨äººè¯†åˆ«å·", $('#invoice_tax_account').val(), "pname", true)) {

        getFocus('invoice_tax_account');

        return false;

    }

    if ($('#invoice_aaddress').val() != undefined && !CheckData2("æ³¨å†Œåœ°å€", $('#invoice_aaddress').val(), "pname", true)) {

        getFocus('invoice_aaddress');

        return false;

    }

    if ($('#invoice_tel').val() != undefined && !CheckData2("æ³¨å†Œç”µè¯", $('#invoice_tel').val(), "pname", true)) {

        getFocus('invoice_tel');

        return false;

    }

    if ($('#invoice_open_bank').val() != undefined && !CheckData2("å¼€æˆ·é“¶è¡Œ", $('#invoice_open_bank').val(), "pname", true)) {

        getFocus('invoice_open_bank');

        return false;

    }

    if ($('#invoice_open_account').val() != undefined && !CheckData2("å¼€æˆ·è´¦å·", $('#invoice_open_account').val(), "pname", true)) {

        getFocus('invoice_open_account');

        return false;

    }

}



function getFocus(thisId) //å¾—åˆ°ç„¦ç‚¹

{

    document.getElementById(thisId).focus();

}



function showWhichOrder(getid) //é€‰æ‹©å“ªç§æ”¯ä»˜æ–¹å¼

{

    if ('wy' == getid)

    {

        document.getElementById('orderForm').action = './pay/bank.php';



    } else if ('zfb' == getid)

    {

        document.getElementById('orderForm').action = './pay/alipay.php';

    } else if ('wx' == getid)

    {

        document.getElementById('orderForm').action = './pay/weixin.php';

    } else if ('hdfk' == getid)

    {

        document.getElementById('orderForm').action = 'huodaofukuan.html';



    } else

    {

        document.getElementById('orderForm').action = 'huikuan.html';



    }

}



