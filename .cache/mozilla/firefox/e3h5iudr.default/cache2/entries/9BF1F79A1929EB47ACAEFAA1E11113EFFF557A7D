var curfontsize=10;
var curlineheight=18;
function turnsmall(){
  if(curfontsize>10){
    getid('content').style.fontSize=(--curfontsize)+'pt';
	getid('content').style.lineHeight=(--curlineheight)+'pt';
  }
}
function turnbig(){
  if(curfontsize<28){
    getid('content').style.fontSize=(++curfontsize)+'pt';
	getid('content').style.lineHeight=(++curlineheight)+'pt';
  }
}
